    "@typescript-eslint/parser": "^5.0.0",
    "common-tags": "^1.8.0",
    "eslint": "^8.0.0",
    "eslint-plugin-fp-ts": "link:./lib",
    "jest": "^26.6.3",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.3"

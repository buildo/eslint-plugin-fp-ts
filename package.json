{
  "name": "eslint-plugin-fp-ts",
  "version": "0.3.0",
  "description": "fp-ts ESLint rules",
  "keywords": ["eslint", "eslintplugin", "eslint-plugin", "fp-ts"],
  "author": "buildo",
  "main": "lib/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/buildo/eslint-plugin-fp-ts"
